<% provide(:title, "All posts") %>
<%= render "shared/heading" %>
<div class="page-content container-fluid">
  <div class="row">
    <div class="col-md-9">
      <div class="post-archive">
        <div class="row">
          <%= render @posts %>
        </div>
      </div>
    </div>
    <div class="col-md-3">
      <%= render('posts/filter') %>
    </div>
  </div>
  <%= will_paginate @posts %>

</div>